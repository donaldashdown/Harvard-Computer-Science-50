//This program will take your first and last name, printing out your initials in upper case.
#include <stdio.h>
#include <string.h>
#include <cs50.h>
#include <ctype.h>


int main(void)

{
    string s = get_string();

    {
    printf("%c", toupper(s[0]));
    }

    for(int i = 0; i < strlen(s); i ++)

        if( s[i] == ' ')


    printf("%c", toupper(s[i + 1]));
    printf("\n");
}

